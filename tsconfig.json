{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ESNext",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "outDir": "./dist",
        "types": [
            "node",
        ],
        "typeRoots": [
            "./node_modules/@types",
            "/src/@types",
        ],
    },
    "include": [
        "src/Pathname.ts",
    ],
    "exclude": [
        "ignore",
    ],
}
